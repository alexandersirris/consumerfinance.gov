env:
  jest: true
  mocha: true
globals:
  cy: true
  Cypress: true
rules:
  complexity:
    - 1
    - 10
  dot-notation: 0
  global-require: 0
  max-lines-per-function: 0
  max-nested-callbacks: 0
  max-statements: 0
  no-unused-expressions: 0
  require-jsdoc: 0
  max-len:
    - 1
    -
      ignorePattern: '^import\s(([A-Za-z0-9]*)|([*]\sas\s[A-Za-z0-9]*))\sfrom\s'
      ignoreComments: true
      ignoreTrailingComments: true
      ignoreUrls: true
      ignoreStrings: false
      ignoreTemplateLiterals: true
      ignoreRegExpLiterals: true 
